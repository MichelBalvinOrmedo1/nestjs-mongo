{
    "$schema": "https://swc.rs/schema.json",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "target": "es2022",
        "baseUrl": "./",
        "paths": {
            "@src/*": [
                "./src/*"
            ]
        }
    },
    "module": {
        "type": "commonjs",
        "resolveFully": true
    },
    "sourceMaps": true,
    "minify": false
}